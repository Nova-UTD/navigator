<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>pedestrian_intent_to_enter_road</name>
  <version>1.0.0</version>
  <description>This node identifies pedestrians and determines whether they are facing the road and how close to the road they are</description>
  <maintainer email="project.nova@utdallas.edu">root</maintainer>
  <license>MIT License</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <depend>rclpy</depend>
  <depend>rclpy.node</depend>
  <depend>rclpy.executors</depend>
  <depend>rclpy.callback_groups</depend>
  <depend>sensor_msgs.msg</depend>
  <depend>cv_bridge</depend>
  <depend>navigator_msgs.msg</depend>
  <depend>cv2</depend>
  <depend>ultralytics</depend>
  <depend>mmpose.apis</depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
