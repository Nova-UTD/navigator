<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>state_estimation</name>
  <version>1.0.0</version>
  <description>Very simple package to convert raw GNSS odometry into a map->base_link transform.</description>
  <maintainer email="project.nova@utdallas.edu">Will Heitman</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <exec_depend>carla_msgs</exec_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
