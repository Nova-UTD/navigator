<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>occupancy_grid</name>
  <version>1.0.0</version>
  <description>Generates occupancy grid based on depth map and segmented images</description>
  <maintainer email="project.nova@utdallas.edu">root</maintainer>
  <license>MIT License</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <depend>rclpy</depend>
  <depend>rclpy.node</depend>
  <depend>nav_msgs.msg</depend>
  <depend>std_msgs.msg</depend>
  <depend>sensor_msgs.msg</depend>
  <depend>cv_bridge</depend>
  <depend>numpy</depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
